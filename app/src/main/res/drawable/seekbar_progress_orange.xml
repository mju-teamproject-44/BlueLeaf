<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- seekbar 배경 1 -->
    <item>
        <!-- 선 : 길이 -->
        <shape android:shape="line">
            <stroke android:width="3dp" android:color="#AAAAAA" />
        </shape>
    </item>

    <!-- seekbar 프로그래스 바 -->
    <item >
        <clip>
            <shape android:shape="line">
                <stroke android:width="4dp" android:color="#FF9F45" />
            </shape>
        </clip>
    </item>

</layer-list>